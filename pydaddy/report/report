    "def decode_fmt(fmt):\n",
    "    c_fmt = {\n",
    "    'X': None,\n",
    "    'Y': None,\n",
    "    'T': None\n",
    "    }\n",
    "    fmt = list(fmt.upper())\n",
    "    if len(fmt) == 0:\n",
    "        c_fmt['X'] = 0\n",
    "    for c in fmt:\n",
    "        c_fmt[c] = fmt.index(c)\n",
    "    return list(c_fmt.values())"
   ]
  },
